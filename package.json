{
  "name": "@superorange/tip-js-sdk",
  "version": "1.0.140",
  "author": "",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build:test": "rm -rf dist && tsc --build tsconfig.json",
    "lint": "eslint -c .eslintrc --ext .ts src",
    "format": "prettier --check --write src/**/*.ts  --config .prettierrc",
    "pretest:integration": "npm run build && npm link",
    "publish": "npm publish --access public",
    "test": "jest",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "build": "rm -rf dist && tsup src/index.ts --format esm,cjs --dts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-package-json": "^2.8.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsup": "^8.4.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@fksyuan/zktls-js-sdk": "^0.3.6",
    "@primuslabs/zktls-js-sdk": "^0.3.3",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.8.1",
    "borsh": "^2.0.0",
    "ethers": "^5.7.2",
    "keccak256": "^1.0.6"
  }
}
